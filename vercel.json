{
  "buildCommand": "cd rabbithole && npm install && npm run build",
  "outputDirectory": "rabbithole/dist",
  "devCommand": "cd rabbithole && npm run dev",
  "installCommand": "cd rabbithole && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "VITE_CONVEX_URL": "https://upbeat-gull-282.convex.cloud"
    }
  }
}
